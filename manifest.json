{
  "name": "Tactify",
  "description": "",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_icon": "images/icon.png"
  },
  "permissions": [
    "activeTab",
    "storage",
    "scripting"
  ],
  "content_scripts":[
    {
        "matches": ["https://www.meet.google.com/*"],
        "js": ["content.js"],
        "run_at": "document_idle"
    }
  ],
  "background": {
    "service_worker": "background.js"
  }
}