{
  "name": "Tactify",
  "description": "An extension for creating a more accessible online space.",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_icon": "images/icon.png"
  },
  "permissions": [
    "storage"
  ],
  "host_permissions":[
    "<all_urls>"
  ]
}